{
  "name": "@tiledb-inc/viz-components",
  "version": "0.0.1",
  "description": "A base template for building a shareable web components library with Vite + Svelte",
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.umd.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "vite build",
    "build:types": "yarn g:tsc --declaration --allowJs false --emitDeclarationOnly",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "storybook": "yarn g:storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.3",
    "@tsconfig/svelte": "^5.0.0",
    "esbuild": "^0.18.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.4.0",
    "typescript": "^5.1.6",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^3.3.1"
  }
}
