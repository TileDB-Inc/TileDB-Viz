{
  "name": "@tiledb-inc/viz-react",
  "version": "0.0.1",
  "description": "React port of @tiledb-inc/viz-core",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "lib/module.js",
  "files": [
    "lib/**/*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TileDB-Inc/TileDB-Viz.git"
  },
  "author": {
    "name": "TileDB",
    "email": "support@tiledb.com"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "targets": {
    "main": {
      "optimize": true
    },
    "module": {
      "optimize": true
    }
  },
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "build": "yarn g:parcel build",
    "storybook": "yarn g:storybook dev -p 6006",
    "build-storybook": "build-storybook",
    "lint": "yarn g:eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "yarn g:eslint './src/**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@tiledb-inc/viz-core": "workspace:packages/core",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@mdx-js/react": "^1.6.22",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.17",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "webpack": "^5.74.0"
  }
}
